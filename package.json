{
  "name": "@bonniernews/eslint-plugin-typescript-rules",
  "version": "1.0.0-alpha.0",
  "description": "ESLint Plugin for preventing non ecmascript compatible patterns in Typescript",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha && eslint .",
    "lint": "npm-run-all \"lint:*\"",
    "lint:js": "eslint .",
    "lint:eslint-docs": "npm run update:eslint-docs -- --check",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BonnierNews/eslint-plugin-typescript-rules"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "bonniernews",
    "javascript",
    "typescript"
  ],
  "contributors": [],
  "devDependencies": {
    "@bonniernews/eslint-config": "^2.0.0-alpha.0",
    "@typescript-eslint/rule-tester": "^8.23.0",
    "eslint": "^9.20.0",
    "eslint-doc-generator": "^2.0.2",
    "mocha": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": ">=9.20",
    "typescript": ">=5.1.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "files": [
    "lib/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
